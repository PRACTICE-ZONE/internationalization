<p style="color: green"><%= notice %></p>

<h1>Feedbacks</h1>

<div id="feedbacks">
  <% @feedbacks.each do |feedback| %>
    <%= render feedback %>
    <p>
      <%= link_to "Show this feedback", feedback, class:"btn btn-secondary" %>
    </p>
  <% end %>
</div>

<%= link_to "New feedback", new_feedback_path, class:"btn btn-secondary" %>
